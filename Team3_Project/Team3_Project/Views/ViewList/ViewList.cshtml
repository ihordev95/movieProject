@{
	ViewBag.Title = "ViewList";
	Layout = "/Views/Shared/_Layout.cshtml";
}			<h2>
				ViewList
			</h2>
			<p>
				This is the page that would show a list selected by the user.
			</p>
			<table>
				<tr>
					<td>
						Identifier
					</td>
					<td>
						@ViewBag.list.identifier
					</td>
				</tr>
				<tr>
					<td>
						User
					</td>
					<td>
						@ViewBag.list.user
					</td>
				</tr>
				<tr>
					<td>
						Name
					</td>
					<td>
						@ViewBag.list.name
					</td>
				</tr>
				<tr>
					<td>
						Hidden
					</td>
					<td>
						@ViewBag.list.hidden
					</td>
				</tr>
			</table>
			<table>
@foreach (System.Data.DataRow DataRow in Model.Tables[0].Rows) {
				<tr>
					<td>
						<a href="/Movie/MoviePage/@DataRow[0]" class="elements">
							@DataRow[1] (@DataRow[2])
						</a>
					</td>
				</tr>
}			</table>