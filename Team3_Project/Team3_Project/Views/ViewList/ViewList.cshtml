@{
	Layout = "/Views/Shared/_Layout.cshtml";
	this.ViewBag.Title = "ViewList";
	this.ViewBag.js = new System.String[] { "ajax" , "event" , "add_list" };
}			<h2>
				ViewList
			</h2>
			<div>
				<input id="list" name="list" readonly="readonly" type="number" value="5" />
				<input id="movie" name="movie" type="text" value="tt0000001" />
				<input id="added" name="added" type="datetime" />
				<button id="send">
					Save
				</button>
			</div>
			<p>
				This is the page that would show a list selected by the user.
			</p>
			<table>
				<tr>
					<td>
						Identifier
					</td>
					<td>
						@ViewBag.list.identifier
					</td>
				</tr>
				<tr>
					<td>
						User
					</td>
					<td>
						@ViewBag.list.user
					</td>
				</tr>
				<tr>
					<td>
						Name
					</td>
					<td>
						@ViewBag.list.name
					</td>
				</tr>
				<tr>
					<td>
						Hidden
					</td>
					<td>
						@ViewBag.list.hidden
					</td>
				</tr>
			</table>
			<table>
@foreach (Team3_Project.Databases.memdixyp_film.view_list view_list in this.ViewBag.view_list) {
				<tr>
					<td>
						<a href="/Movie/MoviePage/@view_list.tconst" class="elements">
							@view_list.primaryTitle (@view_list.startYear)
						</a>
					</td>
				</tr>
}			</table>