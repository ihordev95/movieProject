@{
	this.Layout = "/Views/Shared/_Layout.cshtml";
	this.ViewBag.Title = "ViewList";
	this.ViewBag.css = new System.String[] { "w3" };
}			<h2>
				ViewList
			</h2>
			<table>
				<tr>
					<td>
						Identifier
					</td>
					<td>
						@ViewBag.list.identifier
					</td>
				</tr>
				<tr>
					<td>
						User
					</td>
					<td>
						@ViewBag.list.user
					</td>
				</tr>
				<tr>
					<td>
						Name
					</td>
					<td>
						@ViewBag.list.name
					</td>
				</tr>
				<tr>
					<td>
						Hidden
					</td>
					<td>
						@ViewBag.list.hidden
					</td>
				</tr>
			</table>
			<hr />
			<a class="w3-btn w3-black w3-hover-green w3-round-xxlarge w3-large" href="/ManageList/ManageLists/@ViewBag.list.identifier">
				View List Page
			</a>
			<hr />
			<table>
@foreach (Team3_Project.Databases.memdixyp_film.view_list view_list in this.ViewBag.view_list) {
				<tr>
					<td>
						<a href="/Movie/MoviePage/@view_list.tconst" class="elements">
							<img alt="@view_list.primaryTitle (@view_list.startYear)" src="@view_list.poster" title="@view_list.primaryTitle (@view_list.startYear)" />
						</a>
					</td>
				</tr>
}			</table>