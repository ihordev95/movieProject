@model System.Data.DataSet
@{
	Layout = "/Views/Shared/_Layout.cshtml";
	ViewBag.Title = "MoviePage";
	ViewBag.css = new System.String[] { "w3" , "dynamicmovie-page" };
}<div class="dynamicmovieload w3-mobile">
    <div class="test1">
        <img src=@ViewBag.json.poster alt="A picture of the movie poster" class="movieposter w3-third" />

        <div class="test">
            <button class="w3-btn w3-black w3-hover-green w3-round-xxlarge w3-large" type="submit" id="Save">
                Save
            </button>
            <button class="w3-btn w3-black w3-hover-green w3-round-xxlarge w3-large" type="submit" id="Rate">
                Rate
            </button>
        </div>
    </div>

    <div class="moviedetails w3-mobile w3-twothird">
        <label>Title: @ViewBag.json.title</label><br />
        <label>Year: @ViewBag.json.year</label><br />
        <label>Type: @ViewBag.json.type</label><br />
        <label>Descritpion: @ViewBag.json.plot</label><br />


    </div>
</div>
